{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "atomix-base",
  "rules": {
    "no-magic-numbers": 0,           /* ["warn",
      {
      "ignore": [
          1,
          0
        ],
        "ignoreArrayIndexes": true,
        "enforceConst": true,
        "detectObjects": false
      }
    ], */
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "object-curly-newline":0
  }
}


